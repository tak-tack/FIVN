<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqlmap.study.mappers.UserMapper">


<select id="Login" parameterType="com.epps.study.user.vo.UserVO" resultType="com.epps.study.user.vo.UserVO">
SELECT *
FROM USER
WHERE USERNAME = #{username}
AND PASSWORD = #{password}
</select>


</mapper>